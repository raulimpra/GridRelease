@model IEnumerable<UploadError>

<form asp-action="UploadFile" method="post" enctype="multipart/form-data" class="row needs-validation" novalidate>
    <div class="form-group mb-3 col-md-6 needs-validation">
        <label for="file" class="form-label">Seleccionar Archivo:</label>
        <input type="file" name="file" id="file" class="form-control" required />
        <div class="invalid-feedback">Seleccione un archivo.</div>
    </div>
    <div class="mb-3 col-12">
        <button type="submit" class="btn btn-primary">Subir Archivo</button>
    </div>
</form>

@Html.DisplayForModel("ListUploadError")
@section Scripts{
    <script type="text/javascript" src="~/js/JsViews/User.js" asp-append-version="true"></script>
    <script>
        FileUsers();
    </script>
}