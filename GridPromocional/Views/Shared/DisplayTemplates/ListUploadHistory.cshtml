@model IEnumerable<GridPromocional.Models.PgLogUploadHistory>

<table class="table upload-history" id="TableErrorList" width="100%">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Username)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Filename)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UploadDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalRegisters)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UploadedRegisters)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WrongRegisters)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalErrors)
            </th>
        </tr>
    </thead>
    <tbody>
@if (Model != null)
{
    foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Username)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Filename)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UploadDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalRegisters)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UploadedRegisters)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WrongRegisters)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalErrors)
            </td>
        </tr>
    }
}
    </tbody>
</table>
